# Application settings
APP_ENV=development
LOG_LEVEL=INFO
REQUIRE_API_KEY=false
API_KEYS=demo-key

# LLM configuration
LLM_CONFIG_PATH=config/llm_providers.json
SYSTEM_PROMPT_PATH=prompt/system_recipe.txt
